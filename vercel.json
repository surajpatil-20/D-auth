{
    "builds": [
        {
            "src": "basics/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.12.4" }
        }
    ],
    "routes": [
        {
             "src": "/(.*)",
         "dest": "basics/wsgi.py"
        }
    ]


}